    "rollup-plugin-uglify": "^1.0.1",
    "style-loader": "^0.13.2",
    "typescript": "^2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
