  "description": "Shanoir NG",
  "repository": {
    "type": "git",
    "url": "https://github.com/fli-iam/shanoir-ng.git"
  },
  "scripts": {
    "build": "rimraf dist && webpack --config config/webpack.js --progress --profile --bail",
    "build:aot": "rimraf aot && ngc -p tsconfig-aot.json",
    "build:prod": "npm run build:aot && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:qualif": "npm run build:aot && rimraf dist && webpack --config config/webpack.qualif.js --progress --profile --bail",
    "i18n": "ng-xi18n",
    "start": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "test": "karma start"
  },
  "licenses": [
